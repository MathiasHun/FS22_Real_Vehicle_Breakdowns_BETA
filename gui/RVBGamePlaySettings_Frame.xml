            </GuiElement>
			
			<GuiElement type="threePartBitmap" profile="verticalListSliderRightDocked">
				<GuiElement type="slider" profile="verticalListSliderBar" dataElementId="boxLayout" handleFocus="true" focusInit="onOpen"/>
			</GuiElement>	
			
        </GuiElement>

    </GuiElement>
</GUI>